{
  "name": "flow2src",
  "version": "0.0.0",
  "description": "一个将流程图映射至源码的应用",
  "author": "xlboy",
  "main": "dist/node/index.js",
  "types": "dist/node/index.d.ts",
  "bin": {
    "flow2src": "bin/flow2src.js",
    "f2s": "bin/flow2src.js"
  },
  "scripts": {
    "dev": "concurrently -n client,node \"npm:client-dev\" \"npm:node-dev\"",
    "node-dev": "rimraf dist && concurrently -n rollup,node \"rollup -w -c \" \"nodemon -w dist/node/ -x flow2src\"",
    "client-dev": "cd packages/client && npm run dev",
    "node-build": "rimraf dist && rollup -c && npm run node-build-types",
    "node-build-types": "esno scripts/buildNodeTypes.ts",
    "clean": "rimraf ./**/node_modules",
    "prepare": "husky install"
  },
  "license": "ISC",
  "dependencies": {
    "immer": "^9.0.12",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@microsoft/api-extractor": "^7.19.5",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/jest": "^27.4.0",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.5",
    "@types/koa-router": "^7.4.4",
    "@types/lodash": "^4.14.181",
    "@types/node": "^16.10.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "cac": "^6.7.12",
    "concurrently": "^7.0.0",
    "dotenv": "^15.0.0",
    "esbuild": "^0.14.17",
    "esbuild-jest": "^0.5.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "esno": "^0.14.0",
    "execa": "^6.1.0",
    "fs-jetpack": "^4.3.1",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.7",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.15",
    "picocolors": "^1.0.0",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-esbuild": "^4.8.2",
    "rollup-plugin-terser": "^7.0.2",
    "ts-toolbelt": "^9.6.0",
    "type-fest": "^2.11.0",
    "typescript": "^4.6.2"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js}": [
      "eslint . --ext ts --ext tsx --ext js"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "node": ">= 16.x"
  }
}
